<!--pages/scanresult/index.wxml-->
<view class="container">
    <view class="msg">
        <view class="msg-qx">
            <text>骑行时间</text>
        </view>
        <view class="msg-fy">
            <text>预计费用</text>
        </view>
        <view class="msg-sj">
            <text>{{ride_time}}分</text>
        </view>
        <view class="msg-m">
            <text>{{order_money}}元</text>
        </view>
    </view>
    <view class="msg-num">
        <text class="msg-num-text">正在用车编号：{{bike_code}}</text>
    </view>
    <view class="password-title">
        <text>{{status}}</text>
    </view>
    <!-- <view class="password-content">
        <text>{{password}}</text>
    </view> -->
</view>